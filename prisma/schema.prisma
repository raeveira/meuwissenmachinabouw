generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model POI {
  id            Int     @id @default(autoincrement())
  positionX     Float
  positionY     Float
  imageSrc      String
  header        String
  text          String
  extraImageSrc String?
}

model DynamicText {
  id      Int    @id @default(autoincrement())
  content String
  type    String
}

model POI2 {
  positionX     Float
  positionY     Float
  imageSrc      String
  header        String
  text          String
  id            Int     @id @default(autoincrement())
  extraImageSrc String?
}
