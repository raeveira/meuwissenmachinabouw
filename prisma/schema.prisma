// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model POI {
  id        Int    @id @default(autoincrement())
  positionX Float
  positionY Float
  imageSrc  String
  header    String
  text      String
}

model DynamicText {
  id      Int    @id @default(autoincrement())
  content String
  type    String
}
